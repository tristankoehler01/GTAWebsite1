@page
@model GTAWebsite.Models.Manu.Manu

<h1>Manu Form</h1>

<form method="post">
    <div class="form-group">
        <label for="position">Position:</label>
        <select class="form-control" id="position" name="Position">
            @foreach (var value in Models.Manu.Manu.positions)
            {
                <option value="@value">@value</option>
            }
        </select>
    </div>
    <div class="form-group">
        <label for="qualification">Qualification:</label>
        <select class="form-control" id="qualification" name="Qualification">
            @foreach (var value in Models.Manu.Manu.qualifications)
            {
                <option value="@value">@value</option>
            }
        </select>
    </div>
    <div class="form-group">
        <label for="graderCourses">Grader Courses:</label>
        <select class="form-control" id="graderCourses" name="GraderCourses">
            @foreach (var value in Models.Manu.Manu.courses)
            {
                <option value="@value">@value</option>
            }
        </select>
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>
